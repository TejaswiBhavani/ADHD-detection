(df['TIMESTAMP'].dt.date)[column].std().mean()\n    features[f'{prefix}_entropy'] = -((df[column].value_counts(normalize=True) * np.log2(df[column].value_counts(normalize=True))).sum())\n    \n    return pd.Series(features)\n\n# Apply to activity and HRV files\ntemporal_features = []\nfor file in activity_files + hrv_files:\n    df = pd.read_csv(file, sep=';')\n    prefix = 'ACTIVITY' if 'activity_data' in file else 'HRV'\n    feats = extract_temporal_features(df, prefix, prefix)\n    if feats is not None:\n        feats['ID'] = int(os.path.basename(file).split('_')[-1].split('.')[0])\n        temporal_features.append(feats)\n\nif temporal_features:\n    temporal_df = pd.DataFrame(temporal_features)\n    df_upsampled = df_upsampled.merge(temporal_df, on='ID', how='left')\n    \n    # Impute missing temporal features\n    from sklearn.impute import SimpleImputer\n    num_imputer = SimpleImputer(strategy='mean')\n    temporal_cols = [col for col in temporal_df.columns if col != 'ID']\n    df_upsampled[temporal_cols] = num_imputer.fit_transform(df_upsampled[temporal_cols])\n    \n    print(\"Temporal Features Added:\", temporal_cols)\n    print(\"Updated Upsampled Data Shape:\", df_upsampled.shape)\nelse:\n    print(\"No temporal features extracted due to missing timestamps\")","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-08T15:36:51.687839Z","iopub.execute_input":"2025-05-08T15:36:51.688544Z","iopub.status.idle":"2025-05-08T15:37:10.415104Z","shell.execute_reply.started":"2025-05-08T15:36:51.688517Z","shell.execute_reply":"2025-05-08T15:37:10.414420Z"}},"outputs":[{"name":"stdout","text":"Temporal Features Added: ['ACTIVITY_mean_hourly', 'ACTIVITY_std_hourly', 'ACTIVITY_daily_std', 'ACTIVITY_entropy', 'HRV_mean_hourly', 'HRV_std_hourly', 'HRV_daily_std', 'HRV_entropy']\nUpdated Upsampled Data Shape: (167, 60)\n","output_type":"stream"}],"execution_count":45},{"cell_type":"code","source":"","metadata":{"trusted":true},"outputs":[],"execution_cou